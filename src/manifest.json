{
  "manifest_version": 3,
  "name": "Auto Form Filler",
  "version": "1.0.0",
  "description": "自动点击按钮并填充表单",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/index.js"]
    }
  ]
}
