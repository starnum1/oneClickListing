{
  "manifest_version": 3,
  "name": "一键上架助手",
  "version": "1.0.0",
  "description": "自动点击一键上架按钮",
  "icons": {
    "128": "src/icons/icon128.png"
  },
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "128": "src/icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.ozon.ru/product/*"],
      "js": ["src/content/index.js"]
    }
  ]
}
